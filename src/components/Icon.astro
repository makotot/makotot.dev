---
import { Icon as AstroIcon } from "astro-icon/components"

interface Props {
  name: string
  label?: string
  size: number
}
const {name, label, size} = Astro.props;
---
{
  typeof label === 'string'
  ? 
    <AstroIcon name={name} size={size} aria-label={label} />
  : 
    <AstroIcon name={name} size={size} />
}